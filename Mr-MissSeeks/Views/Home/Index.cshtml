<script>
    function NearestItemsFunc(url) {
        var formDiv = $('<div/>');
        $.get(url)
            .done(function (response) {
                formDiv.html(response);

                PopupItem = formDiv.dialog({
                    autoOpen: true,
                    resizable: false,
                    title: 'Near me',
                    height: 500,
                    width: 700,
                    close: function () {
                        PopupItem.dialog('destroy').remove();

                    }

                });
            });
    }

</script>
